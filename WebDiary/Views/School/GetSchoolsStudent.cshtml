@{
    Layout = "_Layout";
}
@{
    ViewData["Title"] = "Student | Schools";
}
<!DOCTYPE html>
<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Student | School</title>
</head>
<body>
    <div class="columns">
        <div class="column">
            @{

                foreach (School school in Model.GetSchools)
                {
                    @await Html.PartialAsync("_SchoolCardAdvanced", school);
                }

            }
        </div>
       
    </div>
</body>
</html>